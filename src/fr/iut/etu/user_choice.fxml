<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="vbox" fx:id="vbox" alignment="TOP_CENTER" spacing="50" styleClass="user-choice" stylesheets="@style.css" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="label1" styleClass="textMenu" text="Make your choice :" />
        <Button id="button1" fx:id="button1" mnemonicParsing="false" styleClass="button" stylesheets="@style.css" text="Take" />
        <Button id="button2" fx:id="button2" mnemonicParsing="false" styleClass="button" stylesheets="@style.css" text="Keep" />
        <Button id="button3" fx:id="button3" mnemonicParsing="false" styleClass="button" stylesheets="@style.css" text="Keep without dog" />
        <Button id="button4" fx:id="button4" mnemonicParsing="false" styleClass="button" stylesheets="@style.css" text="Keep against dog" />

        <HBox prefHeight="100.0" prefWidth="200.0" />
    </children>

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
